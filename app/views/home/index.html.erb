<h1>Uploaded Latest Images</h1>

<div class="container">
	<div class="row">
		<% @photos.each do |photo|%>
			<% album = Album.find_by_id(photo.album_id)%>
			<div class="col-md-4 col-sm-6">
				<div class="product-grid2">
					<div class="product-image2">
						<%= image_tag photo.image.url, class: "pic-1" %>
					</div>
					<div class="product-content">
						<h3 class="title">

							<%= link_to album.user.name, user_profile_path(album.user.id), style: "color:blue;text-decoration: underline"%>
						</h3>
					</div>
				</div>
			</div>
		<%end%>
	</div>
</div>
